<!--pages/weather.wxml-->
<scroll-view scroll-y class=' weather-content'>

    <swiper class='weather-days'>
      <swiper-item>
      <weather isAddCity='{{}}' inner-text="Some text" bindsearch='search' searchResult='{{searchData}}' ></weather>
      
      </swiper-item>
    
    </swiper>

    <image class='add-city' data-add='true' bindtap='addCity' src='../../assets/weatherImages/add.png'></image>

</scroll-view>